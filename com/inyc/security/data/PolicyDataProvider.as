package com.inyc.security.data {	import com.inyc.security.data.Policy;		/**	* PolicyDataProvider	* 	*/	    public class PolicyDataProvider	{				private var list:Array = new Array();				public function addItem(o:Policy) : void		{			list.push(o);		}				public function getItemAT(num : Number) : Policy		{			return list[num];		}				public function get length() : Number		{			return list.length;		}            }}