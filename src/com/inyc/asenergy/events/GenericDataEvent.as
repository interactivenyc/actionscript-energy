package com.inyc.asenergy.events {	import flash.events.Event;					/**	 * @author stevewarren	 */	public class GenericDataEvent extends Event {		private var _data:*;				public function GenericDataEvent(pType:String, pData:* = null){			super(pType);						_data = pData;		}				public function get data():*{			return _data;		}				public function set data(pData:*):void{			_data = pData;		}			}}