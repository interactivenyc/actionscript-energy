package com.inyc.asenergy.events {		import flash.events.Event;	public class XMLLoaderEvent extends Event 	{				public static const PROGRESS:        String = "onXMLProgress";		public static const COMPLETE:        String = "onXMLComplete";		public static const REFRESH:         String = "onXMLRefresh";		public static const PENDING:         String = "onXMLPending";		public static const ERROR:           String = "onXMLError";				public var data:                     Object;		public function XMLLoaderEvent(pType:String, bubbles:Boolean = false, sData:Object = null) 		{			super(pType, bubbles);			data = sData;					}				public override function toString():String 		{			var eventSent:String;						if (type == PROGRESS) eventSent = formatToString("XMLLoaderEvent", "type", "bubbles", "cancelable", "eventPhase", "data");			else if (type == COMPLETE) eventSent = formatToString("XMLLoaderEvent", "type", "bubbles", "cancelable", "eventPhase", "data");			else if(type == PENDING) eventSent = formatToString("XMLLoaderEvent", "type", "bubbles", "cancelable", "eventPhase", "message");			else if(type == ERROR) eventSent = formatToString("XMLLoaderEvent", "type", "bubbles", "cancelable", "eventPhase", "data");			else eventSent = formatToString("XMLLoaderEvent", "type", "bubbles", "cancelable", "eventPhase");						return eventSent;		}			}	}